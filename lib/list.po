mac List -1
    set return_list list
    add return_list $*
    ret return_list
end

mac List.add 0
    add List.list List.value
end

mac List.init.fill 2
    set List.list list
    set List.value $2
    call for 0 $1 List.add
    del List.value List.times for.i for.expr
    ret List.list
end

mac List.init 1
    zet List.init.value call List.init.fill $1 none
    ret List.init.value
end

mac range.code 0
    add range.list for.i
end

mac range 2
    set range.list []
    call for $1 $2 range.code
    del for.i
    ret range.list
end