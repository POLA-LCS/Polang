mac Pair 3
    method $1 destructor
    method $1 print
    method $1 set_second
    method $1 set_first

    mac destructor 1
        del $1.first $1.second $1.set_first $1.set_second
    end

    mac print 1
        out 'Pair:' . $1.first . $1.second
    end

    mac set_first 2
        set $1.first $2
        assign $1 0 $2
    end

    mac set_second 2
        set $1.second $2
        assign $1 1 $2
    end

    set $1.first $2
    set $1.second $3
    set $1 []
    add $1 $1.first $1.second
end

call Pair par 20 50

call par.print