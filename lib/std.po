mac List -1
    set return_list list
    add return_list $*
    ret return_list
end

mac List.add 0
    add List.list List.value
end

mac List.init.fill 2
    set List.list list
    set List.value $2
    call for 0 $1 List.add
    ret List.list
    del List.list List.value List.times
end

mac List.init 1
    zet List.init.value call List.init.fill $1 none
    ret List.init.value
    del List.init.value
end

mac std::range.code 0
    add std::range.list for.i
end

mac std::range 2
    set std::range.list []
    call for $1 $2 std::range.code
    ret std::range.list
    del std::range.list for.i
end